<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/header.ejs') %>
<body>
    <%- include('./partials/navlogin.ejs') %>
    <!-- <%- include('./partials/sidenav.ejs') %> -->
   
    <div class="prologin">
        <form>
            <h1>Login</h1>
            <label for="name">Name</label>
            <input autocomplete="off" name="name" placeholder="Enter the name" type="text" id="name" required>
            <label for="email">Email</label>
            <input id="email" name="email" placeholder="Enter the Email-id" type="email" required>
            <label for="password">Password</label>
            <input autocomplete="off" name="password" placeholder="Enter the password" type="password" id="password"
                required>
            <button id="login"class="login">Login</button>
        </form>

    </div>
   
</body>

</html>